<?xml version='1.0' encoding='utf-8'?>
<widget id="com.split.app" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Contacts">
        <param name="android-package" value="org.apache.cordova.contacts.ContactManager" />
    </feature>
    <feature name="androidstatusbarcolor">
        <param name="android-package" value="com.internrocket.plugins.statusbarcolor.StatusBarColor" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Keyboard">
        <param name="android-package" value="com.ionic.keyboard.IonicKeyboard" />
        <param name="onload" value="true" />
    </feature>
    <name>Split</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="olivier.tassinari@gmail.com" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <icon src="../logo/v0.png" />
    <access origin="*" />
    <preference name="StatusBarBackgroundColor" value="#388E3C" />
    <preference name="BackgroundColor" value="0xffffffff" />
</widget>
