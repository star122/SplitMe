{
  "name": "SplitMe-app",
  "version": "0.0.22",
  "description": "Split expenses with friends",
  "scripts": {
    "start": "npm run browser:development",
    "browser:development": "grunt build --config=browser.development",
    "browser:release": "NODE_ENV=production grunt dist --config=browser.production",
    "test:unit": "NODE_ENV=test mocha 'src/**/*.test.js' 'test/**/*.test.js'",
    "test:init": "selenium-standalone start",
    "test:feature": "grunt webdriver:feature",
    "android:emulator": "emulator -avd Nexus_5_API_21_x86",
    "android:debug": "grunt dist --config=android.debug && cd cordova && cordova run android --debug --device",
    "android:prerelease": "NODE_ENV=production grunt dist --config=android.prerelease && cd cordova && cordova run android --device --release",
    "android:release": "NODE_ENV=production grunt dist --config=android.production && cd cordova && cordova build android --release"
  },
  "private": true,
  "contributors": [
