[ignore]

# Ignore malformed json
.*/node_modules/npm/.*\.json

.*/node_modules/react-motion/.*
.*/node_modules/react-swipeable-views/src/.*
.*/react-tap-event-plugin/node_modules/fbjs/*
.*/react-side-effect/node_modules/fbjs/*
.*/node_modules/fbjs/lib/minBy.js.flow
.*/node_modules/fbjs/lib/countDistinct.js.flow

[libs]

./node_modules/immutable/dist/immutable.js.flow
./node_modules/fbjs/flow/lib
./flow/interfaces

[options]

suppress_type=$FlowIssue

module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=src
module.ignore_non_literal_requires=true
module.file_ext=.js
module.file_ext=.borwser.js
module.file_ext=.cordova.js
module.file_ext=.server.js

esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable

module.name_mapper='.*\.\(html\|css\|svg\|png\|jpg\|gif\)$' -> '<PROJECT_ROOT>/flow/stubs/url-loader.js'

unsafe.enable_getters_and_setters=true
